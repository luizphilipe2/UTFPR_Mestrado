@INPROCEEDINGS{9474399,
author={Brown, Chris and Parnin, Chris},
booktitle={2021 IEEE/ACM Third International Workshop on Bots in Software Engineering (BotSE)}, title={Nudging Students Toward Better Software Engineering Behaviors},
