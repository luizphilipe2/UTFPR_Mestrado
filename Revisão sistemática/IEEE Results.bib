@INPROCEEDINGS{6934158,
author={Fahrbach, Matthew and Jaromczyk, Jerzy W. and David Mobley, Roy and Moore, Neil},
booktitle={2014 Computer Games: AI, Animation, Mobile, Multimedia, Educational and Serious Games (CGAMES)}, title={Video game bots as a theme for student software competitions},
year={2014},
volume={},
