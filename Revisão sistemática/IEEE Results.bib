@INPROCEEDINGS{9463074,
author={Kinsman, Timothy and Wessel, Mairieli and Gerosa, Marco A. and Treude, Christoph},
booktitle={2021 IEEE/ACM 18th International Conference on Mining Software Repositories (MSR)}, title={How Do Software Developers Use GitHub Actions to Automate Their Workflows?},
