author={Marzuki, Anita and Baba, Mohd Dani},
booktitle={2011 IEEE Control and System Graduate Research Colloquium}, title={Downlink performance evaluation of multi-mode devices in WiMAX and WiFi environment},
year={2011},
volume={},
