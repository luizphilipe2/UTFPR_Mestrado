month={Nov},}
@INPROCEEDINGS{5756926,
author={Weisshardt, Florian and Reiser, Ulrich and Parlitz, Christopher and Verl, Alexander},
booktitle={ISR 2010 (41st International Symposium on Robotics) and ROBOTIK 2010 (6th German Conference on Robotics)}, title={Making High-Tech Service Robot Platforms Available},
year={2010},
volume={},
