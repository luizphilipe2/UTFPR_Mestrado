@INPROCEEDINGS{1286646,
author={Rodriguez-Martinez, M. and Ensenat, J.F. and Pagan, E. and Arzola, J.G. and Sotomayor, M. and Vargas, E.},
booktitle={International Conference on Information Technology: Coding and Computing, 2004. Proceedings. ITCC 2004.}, title={Registration and discovery of services in the NetTraveler integration system for mobile devices},
